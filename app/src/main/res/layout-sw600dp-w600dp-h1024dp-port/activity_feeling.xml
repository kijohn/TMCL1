<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg2"
    tools:context=".feeling">

  <ImageView
      android:id="@+id/imageView7"
      android:layout_width="303dp"
      android:layout_height="258dp"
      android:layout_marginStart="145dp"
      android:layout_marginLeft="145dp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="145dp"
      android:layout_marginRight="145dp"
      android:layout_marginBottom="595dp"
      android:contentDescription="@string/community3"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.034"
      app:srcCompat="@drawable/community1"
      tools:ignore="UnknownId" />

  <ImageView
      android:id="@+id/imageView8"
      android:layout_width="462dp"
      android:layout_height="192dp"
      android:layout_marginStart="35dp"
      android:layout_marginLeft="35dp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="35dp"
      android:layout_marginRight="35dp"
      android:contentDescription="@string/feeling"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView7"
      app:srcCompat="@drawable/feeling" />

  <ScrollView
      android:layout_width="625dp"
      android:layout_height="0dp"
      android:layout_marginStart="146dp"
      android:layout_marginLeft="146dp"
      android:layout_marginTop="16dp"
      android:layout_marginEnd="129dp"
      android:layout_marginRight="129dp"
      android:layout_marginBottom="8dp"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/imageView8">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

      <Button
          android:id="@+id/button14"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/overwhelmed"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button13"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/lonely"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button11"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/jealousy"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button10"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/vengeful_betrayed_angry"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button9"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/feeling_useless"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button8"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/self_doubt"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button7"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/feeling_like_you_don_t_belong"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button6"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/giving_up"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button5"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/discouraged"
          android:textSize="30sp" />

      <Button
          android:id="@+id/button4"
          android:layout_width="match_parent"
          android:layout_height="64dp"
          android:background="@drawable/custom_button"
          android:text="@string/depressed"
          android:textSize="30sp" />
    </LinearLayout>
  </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>